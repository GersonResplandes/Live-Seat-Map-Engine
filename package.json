{
  "name": "live-seat-map-engine",
  "version": "1.0.0",
  "description": "High-concurrency real-time seat serving system using Node.js, Socket.io, and Redis with atomic locking and strict race-condition handling.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "prepare": "husky"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "socket-io",
    "redis",
    "real-time",
    "concurrency",
    "atomic-locking",
    "backend",
    "clean-architecture",
    "websockets"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "winston": "^3.19.0"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ioredis": "^5.9.1",
    "socket.io": "^4.8.3",
    "zod": "^4.3.5"
  }
}
